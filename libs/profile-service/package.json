  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "jsdom": "^26.0.0"
  },
  "peerDependencies": {
    "electron": ">=39.0.0"
  }
}
