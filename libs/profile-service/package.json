{
  "name": "@auto-claude/profile-service",
  "version": "1.0.0",
  "description": "Shared profile service for API credential management",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
